{
  "name": "pc-indec",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n pcINDEC --i18n-format xlf --output-path i18n --i18n-locale de && ng run pcINDEC:xliffmerge",
    "start-es": "ng serve --host localhost --port 4200 --configuration=es --aot",
    "start-en": "ng serve --host localhost --port 44316 --configuration=en --aot",
    "start-all": "concurrently \"npm run start-es\" \"npm run start-en\""
  },
